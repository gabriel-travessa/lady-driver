CORRIDAS_LY2 = 
VAR AnoAnterior = CALCULATE(MAX(dCalendario[Data]), DATEADD(dCalendario[Data], -1, YEAR))
VAR MinLY = MINX(FILTER(dCalendario, dCalendario[Data] = AnoAnterior), dCalendario[Data])
VAR MaxLY = MAXX(FILTER(dCalendario, dCalendario[Data] = AnoAnterior), dCalendario[Data])
RETURN
    CALCULATE (
        SUM ( FRide_ALLTIME[CORRIDAS] ),
        DATESBETWEEN(
            FRide_ALLTIME[Date(r.created_at)],
            MinLY,
            MaxLY
        )
    )
